{
  "name": "ikap2-backend",
  "private": true,
  "version": "0.1.0",
  "scripts": {
    "dev": "concurrently \"npm run dev:backend\" \"npm run dev:frontend\"",
    "dev:backend": "node index.js",
    "dev:frontend": "npm run dev --prefix Frontend",
    "build": "npm install --prefix Frontend --include=dev && npm run build --prefix Frontend",
    "start": "node index.js"
  },
  "dependencies": {
    "@openai/agents": "^0.3.0",
    "cors": "^2.8.5",
    "dotenv": "^17.2.3",
    "express": "^5.1.0",
    "multer": "^2.0.2",
    "openai": "^6.8.1",
    "pg": "^8.16.3",
    "zod": "^3.25.76"
  },
  "devDependencies": {
    "concurrently": "^9.2.1"
  }
}
